generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Movie {
  id            String @id @default(cuid())
  url           String @unique
  originalTitle String
  type          String
  description   String
  startYear     Int
  genre         Json

  createdAt DateTime @default(now())

  @@map("movies")
}
